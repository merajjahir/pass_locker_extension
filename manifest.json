{
    "name" : "Password Locker",
	"version" : "0.1.0",
	"manifest_version" : 3,
	"background": {
		"service_worker": "background.js"
	},

	"action": {
	  "default_popup": "app/index.html",
	  "default_title" : "Passlocker"
	},
	"content_scripts" : [
		{
			"matches" : ["<all_urls>"],
			"js" : ["script.js"]
		}
	]
     
}
